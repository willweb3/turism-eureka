generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["auth", "public"]
}

model User {
  id                                              String        @id @default(uuid()) @db.Uuid
  userType                                        user_role     @map("user_type")
  full_name                                       String
  phone                                           String?
  stripeAccountId                                 String?       @map("stripe_account_id")
  createdAt                                       DateTime?     @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt                                       DateTime?     @default(now()) @updatedAt @map("updated_at") @db.Timestamptz(6)
  listings                                        Listing[]     @relation("ProviderListings")
  users                                           users         @relation(fields: [id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  promoCodes                                      PromoCode[]
  qrCodes                                         QRCode[]
  reviews                                         Review[]
  transactions_transactions_customer_idToprofiles Transaction[] @relation("transactions_customer_idToprofiles")
  transactions_transactions_host_idToprofiles     Transaction[] @relation("transactions_host_idToprofiles")
  transactions_transactions_provider_idToprofiles Transaction[] @relation("transactions_provider_idToprofiles")

  @@index([userType], map: "idx_profiles_user_type")
  @@map("profiles")
  @@schema("public")
}

model Listing {
  id               String            @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  providerId       String            @map("provider_id") @db.Uuid
  type             listing_type
  title            String
  description      String
  pricePerUnit     Int               @map("price_per_unit")
  currency         String            @default("EUR")
  maxCapacity      Int?              @map("max_capacity")
  stock            Int?
  availability     Json?
  images           String[]          @default([])
  status           listing_status    @default(draft)
  createdAt        DateTime?         @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt        DateTime?         @default(now()) @updatedAt @map("updated_at") @db.Timestamptz(6)
  provider         User              @relation("ProviderListings", fields: [providerId], references: [id], onDelete: Cascade, onUpdate: NoAction)
  reviews          Review[]
  transactionItems TransactionItem[]

  @@index([providerId], map: "idx_listings_provider")
  @@index([status], map: "idx_listings_status")
  @@index([type], map: "idx_listings_type")
  @@map("listings")
  @@schema("public")
}

model Transaction {
  id                                          String             @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  customer_id                                 String             @db.Uuid
  provider_id                                 String             @db.Uuid
  host_id                                     String?            @db.Uuid
  promo_code                                  String?
  totalAmount                                 Int                @map("total_amount")
  platform_fee                                Int
  host_commission                             Int                @default(0)
  providerAmount                              Int                @map("provider_amount")
  status                                      transaction_status @default(pending)
  createdAt                                   DateTime?          @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt                                   DateTime?          @default(now()) @updatedAt @map("updated_at") @db.Timestamptz(6)
  reviews                                     Review[]
  items                                       TransactionItem[]
  profiles_transactions_customer_idToprofiles User               @relation("transactions_customer_idToprofiles", fields: [customer_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  profiles_transactions_host_idToprofiles     User?              @relation("transactions_host_idToprofiles", fields: [host_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  profiles_transactions_provider_idToprofiles User               @relation("transactions_provider_idToprofiles", fields: [provider_id], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@index([customer_id], map: "idx_transactions_customer")
  @@index([host_id], map: "idx_transactions_host")
  @@index([provider_id], map: "idx_transactions_provider")
  @@map("transactions")
  @@schema("public")
}

model TransactionItem {
  id                String      @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  transactionId     String      @map("transaction_id") @db.Uuid
  listingId         String      @map("listing_id") @db.Uuid
  quantity          Int         @default(1)
  price_at_purchase Int
  bookingDate       DateTime?   @map("booking_date") @db.Timestamptz(6)
  created_at        DateTime?   @default(now()) @db.Timestamptz(6)
  listing           Listing     @relation(fields: [listingId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  transaction       Transaction @relation(fields: [transactionId], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([listingId], map: "idx_transaction_items_listing")
  @@map("transaction_items")
  @@schema("public")
}

model PromoCode {
  id                  String    @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  hostId              String    @map("host_id") @db.Uuid
  code                String    @unique
  discount_percentage Int       @default(5)
  usage_count         Int       @default(0)
  maxUses             Int?      @map("max_uses")
  valid_until         DateTime? @db.Timestamptz(6)
  active              Boolean   @default(true)
  createdAt           DateTime? @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt           DateTime? @default(now()) @updatedAt @map("updated_at") @db.Timestamptz(6)
  host                User      @relation(fields: [hostId], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([code], map: "idx_promo_codes_code")
  @@index([hostId], map: "idx_promo_codes_host")
  @@map("promo_codes")
  @@schema("public")
}

model QRCode {
  id              String    @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  hostId          String    @map("host_id") @db.Uuid
  code            String    @unique
  name            String
  scanCount       Int       @default(0) @map("scan_count")
  conversionCount Int       @default(0) @map("conversion_count")
  createdAt       DateTime? @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt       DateTime? @default(now()) @updatedAt @map("updated_at") @db.Timestamptz(6)
  host            User      @relation(fields: [hostId], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([code], map: "idx_qr_codes_code")
  @@index([hostId], map: "idx_qr_codes_host")
  @@map("qr_codes")
  @@schema("public")
}

model Review {
  id             String      @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  transaction_id String      @db.Uuid
  listingId      String      @map("listing_id") @db.Uuid
  reviewer_id    String      @db.Uuid
  rating         Int
  comment        String?
  createdAt      DateTime?   @default(now()) @map("created_at") @db.Timestamptz(6)
  listing        Listing     @relation(fields: [listingId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  profiles       User        @relation(fields: [reviewer_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  transactions   Transaction @relation(fields: [transaction_id], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@index([listingId], map: "idx_reviews_listing")
  @@map("reviews")
  @@schema("public")
}

model Commission {
  id                      String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  transaction_id          String    @unique @db.Uuid
  host_id                 String?   @db.Uuid
  provider_id             String?   @db.Uuid
  platform_amount_cents   Int
  host_amount_cents       Int       @default(0)
  provider_amount_cents   Int
  status                  String    @default("pending")
  paid_at                 DateTime? @db.Timestamptz(6)
  created_at              DateTime? @default(now()) @db.Timestamptz(6)

  @@index([host_id], map: "idx_commissions_host")
  @@index([provider_id], map: "idx_commissions_provider")
  @@index([transaction_id], map: "idx_commissions_transaction")
  @@map("commissions")
  @@schema("public")
}

